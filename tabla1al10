using System;

namespace tablas1al10
{
    class Program
    {
        static void Main(string[] args)

        {
            int tabla;

            Console.WriteLine("ingrese tabla de multiplicar que quiere imprimir");
            tabla = Convert.ToInt32(Console.ReadLine());

            for(int i = 1; i <= 10; i++)
            {
                Console.WriteLine(tabla + "x" + i +"=" + tabla * i);
            }
            Console.ReadKey();

            
        }
        
    }
}
